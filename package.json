{
  "name": "babel-transform-dir",
  "version": "1.0.0",
  "description": "babel-transform-dir",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "test": "npm run build && node --harmony ./node_modules/.bin/ava --verbose --timeout=10s",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/babel-transform-dir.git"
  },
  "keywords": [
    "babel-transform-dir"
  ],
  "engines": {
    "node": ">=4"
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/babel-transform-dir/issues"
  },
  "ava": {
    "require": "babel-register",
    "babel": {
      "babelrc": true
    },
    "files": [
      "test/*.js"
    ]
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-polyfill": "^6.23.0"
  },
  "dependencies": {
    "babel-core": "^6.24.0",
    "fs-promise": "^2.0.0",
    "globby": "^6.1.0"
  }
}
